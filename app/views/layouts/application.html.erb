<!DOCTYPE html>
<html>
	<head>
		<title>DeviseDemoS</title>
		<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
		<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
		<%= csrf_meta_tags %>
	</head>
	<body>
		<%= render 'layouts/header'%>
		<%#= link_to "Sign in with Linked in", user_omniauth_authorize_path(:provider) %>
		<% if current_user %>
			<%= image_tag(current_user.largeimage)%>
		<% end %><br>
		Name : <b><%= $auth_hash.info.name%></b><br>
		User id :<b><%= $auth_hash.uid%></b><br>
		Email :<b><%= $auth_hash.info.email%></b><br>
		Provider : <b><%= $auth_hash.provider%></b><br>

		<p class="notice"><%= notice %></p>
		<p class="alert"><%= alert %></p>
		<%= yield %>
		
	</body>
</html>